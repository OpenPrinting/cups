<SCRIPT TYPE="text/javascript"><!--
function filterItems(el) {
  var value = el.value.toLowerCase();
  var form = el.form;
  var sel = form.PPD_NAME;
  var opt = sel.options;

  for (var i=0; i<opt.length; i++) {
    if (value && opt[i].text.toLowerCase().indexOf(value) == -1) {
      opt[i].hidden = true;
    } else {
      opt[i].hidden = false;
    }
  }
}
--></SCRIPT>

